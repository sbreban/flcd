S S
T a
T b
NT A
NT S
P S->AA
P A->aA
P A->b
end

S program
NT program
NT decllist
NT cmpdstmt
NT stmtlist
NT declaration
NT type
NT cmpdstmt
NT stmt
NT simplstmt
NT assignstmt
NT expression
NT term
NT factor
NT iostmt
NT structstmt
NT ifstmt
NT whilestmt
NT condition
NT relation
T int
T boolean
T char
T [
T ]
T {
T }
T (
T )
T ;
T +
T *
T <-
T -
T if
T while
T /
T else
T <
T <=
T ==
T !=
T >=
T >
T a
T read
P program->decllist;cmpdstmt
P declaration->typea
P decllist->declaration
P decllist->declaration;decllist
P type->char
P type->int
P type->boolean
P cmpdstmt->{stmtlist}
P stmtlist->stmt
P stmtlist->stmt;stmtlist
P stmt->simplstmt
P stmt->structstmt
P simplstmt->assignstmt
P simplstmt->iostmt
P assignstmt->identifier<-expression
P expression->term
P expression->term+expression
P expression->term-expression
P term->factor
P term->factor*term
P term->factor/term
P factor->number
P factor->identifier
P factor->(expression)
P iostmt->read(a)
P iostmt->write(expression)
P structstmt->cmpdstmt
P structstmt->ifstmt
P structstmt->whilestmt
P ifstmt->if(condition)stmtelsestmt
P ifstmt->if(condition)stmt
P whilestmt->while(condition)stmtlist
P condition->expressionrelationexpression
P condition->expression
P relation-><
P relation-><=
P relation->==
P relation->!=
P relation->>=
P relation->>
end

S E
T id
T (
T )
T +
NT E
NT T
P E->T
P E->E+T
P T->id
P T->(E)
end

S S
T a
T b
T c
NT S
NT A
P S->aA
P A->cS
P A->c
P S->empty
end
S S
T 0
T 1
NT S
NT T
NT O
NT I
P S->OT
P S->OI
P T->SI
P O->0
P I->1
end
